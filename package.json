{
    "name": "logseq13-full-house-plugin",
    "version": "1.0.0",
    "description": "Templates you will really love ❤️",
    "author": "stdword",
    "repository": "https://github.com/stdword/logseq13-full-house-plugin.git",
    "license": "MIT",
    "logseq": {
        "id": "logseq13-full-house",
        "title": "Full House Templates",
        "icon": "./icon.png",
        "main": "./dist/index.html"
    },
    "scripts": {
        "preinstall": "npx only-allow pnpm",
        "clean": "rm -r ./dist/* || true",
        "dev": "vite",
        "test": "ts-doctest ./ ./tests && jest",
        "build-assets": "cp ./assets/* ./dist && cp package.json ./dist",
        "build": "tsc && vite build --mode=dev && npm run build-assets",
        "prod": "npm run clean && vite build && npm run build-assets"
    },
    "dependencies": {
        "@logseq/libs": "^0.0.14",
        "dayjs": "^1.11.7",
        "eta": "^2.0.1",
        "logseq-dateutils": "^0.0.26"
    },
    "devDependencies": {
        "@semantic-release/changelog": "^6.0.2",
        "@semantic-release/exec": "^6.0.3",
        "@semantic-release/git": "^10.0.1",
        "@types/node": "^18.14.6",
        "conventional-changelog-conventionalcommits": "^5.0.0",
        "cz-conventional-changelog": "^3.3.0",
        "jest": "^29.4.3",
        "semantic-release": "^20.1.1",
        "typescript": "^4.9.5",
        "typescript-doctest": "^1.1.0",
        "vite": "^4.1.4",
        "vite-plugin-logseq": "^1.1.2"
    },
    "config": {
        "commitizen": {
            "path": "cz-conventional-changelog"
        }
    }
}
