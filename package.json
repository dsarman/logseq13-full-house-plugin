{
    "name": "logseq13-full-house-plugin",
    "version": "2.6.0",
    "description": "Templates you will really love ❤️",
    "keywords": [
        "templates",
        "schemas",
        "forms",
        "fill-data",
        "views",
        "javascript",
        "meta-data",
        "meta-info",
        "logic"
    ],
    "author": "stdword",
    "repository": "https://github.com/stdword/logseq13-full-house-plugin.git",
    "license": "MIT",
    "logseq": {
        "id": "logseq13-full-house",
        "title": "Full House Templates",
        "icon": "./icon.png",
        "main": "./dist/index.html"
    },
    "scripts": {
        "preinstall": "npx only-allow pnpm",
        "clean": "rm -r ./dist/* || true",
        "dev": "vite",
        "test": "jest",
        "build": "tsc && vite build --mode=dev",
        "prod": "npm run clean && vite build"
    },
    "dependencies": {
        "@logseq/libs": "^0.0.15",
        "dayjs": "^1.11.7",
        "eta": "^2.1.1",
        "mldoc": "^1.5.3"
    },
    "devDependencies": {
        "@semantic-release/changelog": "^6.0.3",
        "@semantic-release/exec": "^6.0.3",
        "@semantic-release/git": "^10.0.1",
        "@types/jest": "^29.5.1",
        "@types/node": "^20.1.3",
        "conventional-changelog-conventionalcommits": "^5.0.0",
        "cz-conventional-changelog": "^3.3.0",
        "jest": "^29.5.0",
        "semantic-release": "^21.0.2",
        "ts-jest": "^29.1.0",
        "typescript": "^5.0.4",
        "vite": "^4.3.5",
        "vite-plugin-logseq": "^1.1.2"
    },
    "config": {
        "commitizen": {
            "path": "cz-conventional-changelog"
        }
    }
}
